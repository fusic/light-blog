<div class="alignleft">
  <h1><a href="<%- config.root %>"><img src="/favicon.png" height="30" style="margin:0 5px 2px 0; vertical-align: bottom;"><%= config.title %></a></h1>
  <h2><a href="<%- config.root %>" style="margin: 55px;"><% if (config.subtitle){ %><%= config.subtitle %><% } %></a></h2>
</div>
<nav id="main-nav" class="alignright">
  <ul>
    <% for (var i in theme.menu){ %>
      <li><a href="<%- config.root %><%- theme.menu[i] %>"><%= i %></a></li>
    <% } %>
  </ul>
  <div class="clearfix"></div>
</nav>
<div class="clearfix"></div>
